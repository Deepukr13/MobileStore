@model MobileStore.WebUI.Models.ProductsListViewModel 

@{ 

    ViewBag.Title = "Products"; 
} 

@foreach (var p in Model.Products) { 
    @Html.ActionLink(p.ProductName, "Details", new { p.ProductID }, null)
    Html.RenderPartial("ProductSummary", p); 
} 

<div class="pager"> 

    @Html.PageLinks(Model.PagingInfo, x => Url.Action("Blist", new {page = x, category = Model.CurrentCategory})) 

</div>